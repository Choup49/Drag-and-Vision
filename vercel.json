
{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.tsx",
      "use": "@vercel/static"
    },
    {
      "src": "*.ts",
      "use": "@vercel/static"
    },
    {
      "src": "components/**",
      "use": "@vercel/static"
    },
    {
      "src": "services/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
